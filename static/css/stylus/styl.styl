bodyFont = 'Open Sans', sans-serif


/*@font-face*/

@font-face {
    font-family: 'icomoon';
    src:url('../fonts/icomoon.eot?-vup9ry');
    src:url('../fonts/icomoon.eot?#iefix-vup9ry') format('embedded-opentype');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'icomoon';
    src:url('../fonts/icomoon.woff?-vup9ry') format('woff');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'icomoon';
    src:url('../fonts/icomoon.ttf?-vup9ry') format('truetype');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'icomoon';
    src:url('../fonts/icomoon.svg?-vup9ry#icomoon') format('svg');
    font-weight: normal;
    font-style: normal;
}


