body
	font-family bodyFont
	color c_bodyFont
	font-smoothing antialiased
	-webkit-font-smoothing antialiased
	
a
	&:hover 
		color afic
		

// Heeader

.Header-container
	background rgba(#716b5c,1)
	height 80px

.Header-logo
	display inline-block
	position absolute
	top 3px
	left 15px
	padding 7px 0	
	coursor pointer 

.MainMenu-icon 
	font-size 2.5em
	display inline-block
	position absolute
	top 10px 
	right 15px
	
.icon-menu
	float right
	color white
	&:before
		font-family icomoon
	
.MainMenu-container
	background secundariDark
	text-align center
	padding 10px 0  
	margin 0

.MainMenu-option
	display inline-block
	width 100%
	padding 15px 0
	
.MainMenu-link
	color white
	text-decoration none
	font-weight 600
	font-size 16px
	display block

.MainMenu
.MainSection-message
	display none
	

// MainSection

.MainSection
	background url(../img/hero/home-small.jpg)
	background-repeat no-repeat
	background-position-x 90% 
	background-position-y top
	background-size cover
	padding 60px 0 80px 0
	
.MainSection-title
	display inline-block
	color white
	font-weight 600
	text-shadow 0 0 10px rgba(black,.3)
	margin 0
	padding 70px 0 40px 0
	font-size 27px
	line-height 34px
	
.MainSection-cta
	display inline-block
		
	

// NextEvent

.NextEvent
	background secundariLight
	padding 2em 0 
	overflow hidden

.NextEvent-date
	vertical-align top
	display inline-block
	width 15%
	text-align center
	padding 0 0 0 0 

.NextEvent-dateMonth
	font-size 1.3em

.NextEvent-dateDay
	font-size 2.75em
	font-weight 600
	line-height 1em
	
.NextEvent-content
	display inline-block
	padding 0 0 0 1em
	width 80%

.NextEvent-label
	background primari
	color white
	padding .2em .3em
	
.NextEvent-title
	font-size 1.125em
	font-weight 600
	line-height 1em
	padding .3em 0 0 0 

.NextEvent-detail
	padding 1em 0 0 0
	display none

.NextEvent-description
	margin 0 


// CourseBanner

.CourseBanner
	padding 30px 0 10px 0

.CourseBanner-title
	font-size 1.5em
	text-align center
	line-height 1.3em 
	font-weight 600
	margin 0 0 20px 0
	
.CourseBanner-description
	text-align center
	margin 0
	font-size 16px
	line-height 23px



// CourseNavigation
	
.CourseNavigation
	width 100%
	margin 0 auto
	display block
	padding 30px 0 10px 0

.CourseNavigation-container
	padding 0

.CourseNavigation-list
	margin 0
	text-align center
	dd
		display inline-block
		margin 0 .5rem .5rem 0
		transition .1s
		-webkit-transition .1s
		-webkit-filter: brightness(1);
		&:hover
			-webkit-filter: brightness(1.2);
		a
			color white
			&:hover
				color white


// CourseList

.CourseList
	padding 35px 0 70px 0

.CourseList-list
	padding 0
	margin 0
	
.CourseList-course
	position relative
	display inline-block
	border-bottom 1px solid rgba(black,.3)
	width 100%
	padding 25px 0 35px 0

.CourseList-courseData
	display inline-block
	position absolute
	top -20px
	
.CourseList-courseInfo
	display inline-block
	margin 0 0 0 80px

.CourseList-courseTitle
	font-size 1.315em
	font-weight 600
	line-height 1.2em
	margin 0 0 .5em 0
	max-width 120px

.extraHeight
	margin 0 0 25px 0

.CourseList-courseGoblalLink
	position absolute
	top 0
	right 0
	bottom 0
	left 0

.spanLink
	color primari
	font-weight 600
	display block
	width 140px
	&:after
		font-family icomoon
		content: "\e600";
		position relative
		font-size 20px
		top 3px	
	&:hover
		color afic
		
