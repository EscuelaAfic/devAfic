// Mailchimp

.Newsletter
	display none	


// Footer

.Footer
	background secundariDark
	padding 30px 0 460px 0px
	color white
	text-align center
	
.Footer-columnSmall
	display inline-block
	width 100%
	font-weight 600
	border-bottom 1px solid rgba(white,.1)
	padding 0 0 30px 0
	
.Footer-columnBig
	margin 30px 0 0 0
	display inline-block
	width 100%
	text-align left

.Footer-contactInfo
	display block
	max-width 270px
	text-align left
	margin 0 0 0 0 
	line-height 8px
	p
		font-weight 600
		margin 0 0 0 30px
	&:before
		position relative
		top 18px
		margin 0 
		font-size 20px

.Footer-nav
	margin 10px 0 0 0 

.Footer-navList
	text-align left
	padding 0 0 0 1rem
	margin 0 0 0 0 

.Footer-navLink
	display inline-block
	margin 0 0 0 0 
	display list-item
	list-style-type 
	&:first-child
		margin 0
	a
		color white
		font-size 1rem
		&:after
			font-family icomoon
			content: "\e600";
			position relative
			font-size 20px
			top 3px	
		&:hover
			color afic

.Footer-logo
	display inline-block

.Footer-extraInfo
	text-align left
	margin 1em 0 0 0
	p
		font-weight 600

.Footer-cta
	display inline-block
	width 100%

.Footer-network
	display inline-block

.Footer-networkTitle
	margin 1em 0 0 0 
	display inline-block
	h4
		display inline-block
		color white
		font-weight 600
		border-right 1px solid white
		padding 0 1em 0 0 
		font-size 1.125rem

.Footer-networkList
	display inline-block

.Footer-networkLink
	display inline-block
	font-size 20px
	color white
	vertical-align middle

	

