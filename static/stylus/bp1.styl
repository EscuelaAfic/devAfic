// Footer

.Footer
	background secundariDark
	padding 30px 0 460px 0px
	color white
	text-align center
	
.Footer-columnSmall
	display inline-block
	width 100%
	font-weight 600
	border-bottom 1px solid rgba(white,.1)
	padding 0 0 30px 0
	
