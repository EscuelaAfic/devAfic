// Index

.Index
	background terciariLight
	
.Index-img
	img
		width 100%
		position relative
		
.Index-listToggle
	border-bottom 1px solid c_borde
	padding 10px 0
	&:hover
		background rgba(black,.05)
		
.Index-list
	background white
	padding 20px 0 10px 25px
	display none
	
.Index-intraList
	padding 0 0 0 15px
	list-style-type disc 

.Index-listToggle
	font-size 21px 
	letter-spacing -1px
	font-weight 600
	padding 10px 0 10px 10px
	position relative

.Index-listToggleIcon
	position absolute
	right 40px
	font-size 24px
	top 10px

.Index-listToggleIconBook
	&:after
		font-family icomoon
		content "\e920"
		border-right 1px solid
		padding 0 10px 0 0 
		
.Index-listToggleIconArrow
	transform: rotate(90deg)
	-webkit-transform: rotate(90deg)
	-ms-transform: rotate(90deg)
	position relative
	top -30px
	right -35px
	&:before
		font-family icomoon
		content "\e600"
		line-height 24px
		
.ListOne-toggle
	color #88b035
	span
		color #88b035
		
.ListTwo-toggle
	color #378b37
	span
		color #378b37
		
.ListThree-toggle
	color #12767a
	span
		color #12767a
		
.index-item
	display block
	


