.MainSection
	background url(../img/hero/home-big.jpg)
	background-repeat no-repeat
	background-position center top
	background-size cover
	padding 140px 0 140px 0
	position relative
	z-index 0
	animation animationHero 20s infinite
	-webkit-animation animationHero 20s infinite
	
@keyframes animationHero
	0%
		background url(../img/hero/home-big.jpg)
		background-repeat no-repeat
		background-position center top
		background-size cover
		padding 140px 0 140px 0
		position relative
		z-index 0
	33%
		background url(../img/hero/home2-big.jpg)
		background-repeat no-repeat
		background-position center top
		background-size cover
		padding 140px 0 140px 0
		position relative
		z-index 0
	66%
		background url(../img/hero/home3-big.jpg)
		background-repeat no-repeat
		background-position center top
		background-size cover
		padding 140px 0 140px 0
		position relative
		z-index 0
	100%
		background url(../img/hero/home-big.jpg)
		background-repeat no-repeat
		background-position center top
		background-size cover
		padding 140px 0 140px 0
		position relative
		z-index 0


